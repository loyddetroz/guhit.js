{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "target": "ES6",
    "moduleResolution": "Node",
    "declaration": true,
    "isolatedModules": true,
    "noEmit": true,
    "rootDir": ".",
    "outDir": "build",
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts",
  ],
  "exclude": [
    "node_modules",
    "src/**/*.test.ts"
  ]
}
